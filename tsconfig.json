{
  "extends": "astro/tsconfigs/base",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@components/*": [
        "./src/components/*"
      ],
      "@layouts/*": [
        "./src/layouts/*"
      ],
      "@pages/*": [
        "./src/pages/*"
      ],
      "@styles/*": [
        "./src/styles/*"
      ],
      "@assets/*": [
        "./src/assets/*"
      ],
      "@i18n/*": [
        "./src/i18n/*"
      ],
      "@icons/*": [
        "./src/icons/*"
      ],
      "@screens/*": [
        "./src/screens/*"
      ],
      "@constants/*": [
        "./src/constants/*"
      ]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  }
}